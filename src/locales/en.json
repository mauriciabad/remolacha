  "characters": {
    "c:werewolf_name": "Werewolf",
    "c:werewolf_description": "During the night, you vote someone to be killed with the other werewolves in secret.",
    "c:lone-werewolf_name": "Lone werewolf",
    "c:lone-werewolf_description": "Same as @:c:werewolf_name but has to be the last werewolve alive when the werewolves win in order to win.",
    "c:villager_name": "Villager",
    "c:villager_description": "Nothing.",
    "c:nudist_name": "Nudist",
    "c:nudist_description": "Everyone knows that you are the nudist.",
    "c:seer_name": "Seer",
    "c:seer_description": "You can know the role of a player every night.",
    "c:suicide_name": "Suicide",
    "c:suicide_description": "If you get killed in the first voting session, you win. Otherwise, you become a regular villager.",
    "c:feral-kid_name": "Feral kid",
    "c:feral-kid_description": "The first night, you choose your adoptive parent. You start as a villager, but when your parent dies, you become a werewolf.",
    "c:baker_name": "Baker",
    "c:baker_description": "Every night, you wake someone up and reveal your role in secret.",
    "c:triplet_name": "Triplet",
    "c:triplet_description": "You know who your siblings are.",
    "c:twin_name": "Twin",
    "c:twin_description": "You know who your siblings are.",
    "c:witch_name": "Witch",
    "c:witch_description": "Can save 1 person that got killed during the night before it finishes, also can kill 1 person at the end of the night. 1 usage of each.",
    "c:doctor_name": "Doctor",
    "c:doctor_description": "Can save 1 person that got killed during the night before it finishes. 1 usage.",
    "c:scarecrow_name": "Scarecrow",
    "c:scarecrow_description": "Can't be killed by the werewolves. If they try to kill him, his death won't be anounced on the next morning."
  },
  "abilities": {
    "a:nothing_name": "Nothing",
    "a:nothing_description": "No ability.",
    "a:extra-life_name": "Extra life",
    "a:extra-life_description": "When your death is announced, you can use the extra life and not die. 1 usage.",
    "a:extra-vote_name": "Extra vote",
    "a:extra-vote_description": "In one voting session, you can emit another vote. 1 usage.",
    "a:cupid_name": "Cupid",
    "a:cupid_description": "The first night, you have to choose two players to fall in love. If one lover dies, the other also dies.",
    "a:major_name": "Major",
    "a:major_description": "In case of a tie during the day, your vote defines the result. When a major dies, he chooses the next major, this way there's always a major.",
    "a:reveal-ability_name": "Detective",
    "a:reveal-ability_description": "During the day, you can publicly reveal a player's ability. 1 usage.",
    "a:extra-life-key_name": "Heart key",
    "a:extra-life-key_description": "When a player with a {'\"'}@:abilities.a:extra-life-lock_name{'\"'} dies, you can revive him. 1 usage.",
    "a:extra-life-lock_name": "Heart lock",
    "a:extra-life-lock_description": "When you die, if a player with a {'\"'}@:abilities.a:extra-life-key_name{'\"'} wants to save you, you revive. 1 usage.",
    "a:cancel-revive_name": "Spoilsport",
    "a:cancel-revive_description": "When someone is reviced you can cancel it. 1 usage.",
    "a:save-someone-else_name": "Mouth-to-mouth",
    "a:save-someone-else_description": "When someone dies, you can save him. You cannot save yourself. 1 usage.",
    "a:steal-ability_name": "Ability thief",
    "a:steal-ability_description": "During the day, you can see 2 player's abilities and steal 1. 1 usage.",
    "a:poison_name": "Poison",
    "a:poison_description": "During the day, you can publicly poison someone. That player is going to die at the next night's ending. 1 usage",
    "a:protect_name": "Protector",
    "a:protect_description": "During the day, you can publicly protect someone against the werewolves. That player can't be killed by the werewolves the next night. 1 usage",
    "a:beatboxer_name": "Beatboxer",
    "a:beatboxer_description": "During the night, practices beatbox so that villagers cannot hear noises.",
    "a:collejas_name": "The slap-man",
    "a:collejas_description": "During the night, signals the storyteller who to slap on the back of the neck and with what power (from 1 to 5).",
    "a:handcuffs_name": "Handcuffs",
    "a:handcuffs_description": "During the day, you can publicly send someone to jail until the next morning. 1 usage.",
    "a:secret-keeper_name": "Secret keeper",
    "a:secret-keeper_description": "When someone dies, including you, you can see their character and decide whether to reveal it, keep it a secret or lie. 1 usage.",
    "a:nose-club_name": "Nose club",
    "a:nose-club_description": "During the day, you can discreetly put your finger on your nose and others will have to do so. The last one to touch their nose must discard his abilities without revealing them. 1 use.",
    "a:president-call_name": "President calls",
    "a:president-call_description": "During the day, you decide when the president calls. The narrator will tell you a code (word or phrase) and you will say it from ear to ear, when everyone has heard the code they will say it one by one out loud and in reverse order. If the key has been transmitted correctly, the werewolves will not wake up the next night. 1 usage.",
    "a:vital-agreement_name": "Vital agreement",
    "a:vital-agreement_description": "When someone dies, if the other person with this ability also wants, you can revive them. 1 usage.",
    "a:ghost_name": "Ghost",
    "a:ghost_description": "Every night chooses a person to be scared by the storyteller."
  },
