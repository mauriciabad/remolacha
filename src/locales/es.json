  "characters": {
    "c:werewolf_name": "Hombre lobo",
    "c:werewolf_description": "Durante la noche, votas a que jugador matar junto a los otros hombres lobo en secreto.",
    "c:lone-werewolf_name": "Hombre lobo solitario",
    "c:lone-werewolf_description": "Igual que @:c:werewolf_name, pero tiene que ser el último hombre lobo vivo cuando ganen para ganar.",
    "c:villager_name": "Aldeano",
    "c:villager_description": "Nada.",
    "c:nudist_name": "Nudista",
    "c:nudist_description": "Todos saben que eres nudista.",
    "c:seer_name": "Vidente",
    "c:seer_description": "Puedes conocer el personaje de un jugador cada noche.",
    "c:suicide_name": "Suicida",
    "c:suicide_description": "Si te matan en la primera elección, ganas. Si no, pasas a ser un aldeano normal.",
    "c:feral-kid_name": "Niño salvaje",
    "c:feral-kid_description": "La primera noche escoges a tu padre/madre adoptivo. Empiezas siendo un aldeano, pero cuando esa persona muere, te transformas en hombre lobo.",
    "c:baker_name": "Panadero",
    "c:baker_description": "Al final de cada noche despiertas a alguien y le revelas tu personaje.",
    "c:triplet_name": "Trilliza",
    "c:triplet_description": "Sabes quienes son tus hermanas.",
    "c:twin_name": "Melliza",
    "c:twin_description": "Sabes quienes son tus hermanas.",
    "c:witch_name": "Bruja",
    "c:witch_description": "Puedes revivir una persona matada durante la noche antes de que termine. O también puedes matar a una persona al final de la noche.  Solo puedes hacer cada cosa una vez.",
    "c:doctor_name": "Doctora",
    "c:doctor_description": "Puedes revivir una persona matada durante la noche, antes de que termine. 1 uso.",
    "c:scarecrow_name": "Hombre de paja",
    "c:scarecrow_description": "No puede ser matado por los hombres lobo. Si lo intentan matar, su muerte no será anunciada la siguente mañana."
  },
  "abilities": {
    "a:nothing_name": "Nada",
    "a:nothing_description": "Nada.",
    "a:extra-life_name": "Vida extra",
    "a:extra-life_description": "Cuando se anuncia tu muerte, puedes usar la vida extra y no morir. 1 uso.",
    "a:extra-vote_name": "Voto extra",
    "a:extra-vote_description": "En una elección, puedes emitir un voto más. 1 uso.",
    "a:cupid_name": "Cupido",
    "a:cupid_description": "La primera noche eliges a dos jugadores y estos se enamoran. El enamoramiento causa que si uno muere, el otro también.",
    "a:major_name": "Alcalde",
    "a:major_description": "En cado de empate, tu voto determina el resultado. Cuando el alcalde muere, escoge al siguiente alcalde sucesor, de forma que siempre hay alcalde.",
    "a:reveal-ability_name": "Detective",
    "a:reveal-ability_description": "Durante el dia, puedes revelar publicamente una habilidad de un jugador. 1 uso.",
    "a:extra-life-key_name": "Llave corazón",
    "a:extra-life-key_description": "Cuando un jugador con el {'\"'}@:abilities.a:extra-life-lock_name{'\"'} muere, puedes revivirlo. 1 uso.",
    "a:extra-life-lock_name": "Candado corazón",
    "a:extra-life-lock_description": "Cuando mueras, muestra esta habilidad y si un jugador con una {'\"'}@:abilities.a:extra-life-key_name{'\"'} quiere salvarte, revives. 1 uso.",
    "a:cancel-revive_name": "Aguafiestas",
    "a:cancel-revive_description": "Cuando alguien sea revivido, puedes cancelarlo. 1 uso.",
    "a:save-someone-else_name": "Boca a boca",
    "a:save-someone-else_description": "Cuando muera alguien, puedes salvarlo. No puedes salvarte a ti mismo. 1 uso.",
    "a:steal-ability_name": "Ladrón de habilidades",
    "a:steal-ability_description": "Durante el dia, puedes mirar 2 habilidades de otros jugadores y robar 1 de ellas. 1 uso.",
    "a:poison_name": "Veneno",
    "a:poison_description": "Durante el dia, puedes envenenar publicamente a un jugador. Este jugador morirá justo al terminar la siguiente noche. 1 uso.",
    "a:protect_name": "Protector",
    "a:protect_description": "Durante el dia, puedes proteger publicamente a un jugador. Este jugador no podra ser matado por los hombres lobo la siguiente noche. 1 uso.",
    "a:beatboxer_name": "Beatboxer",
    "a:beatboxer_description": "Durante la noche, practica beatbox para que los aldeanos no escuchen ruidos.",
    "a:collejas_name": "El collejas",
    "a:collejas_description": "Durante la noche, indica al narrador a quien meterle una coleja y con que intensidad (del 1 al 5).",
    "a:handcuffs_name": "Esposas",
    "a:handcuffs_description": "Durante el dia, puedes mandar publicamente a una persona a la cárcel hasta la mañana siguiente. 1 uso.",
    "a:secret-keeper_name": "Guarda-Secretos",
    "a:secret-keeper_description": "Cuando alguien muere, incluido tu, puedes ver su personaje y decidir si revelarlo, mantenerlo en secreto o mentir. 1 uso.",
    "a:nose-club_name": "Club de la nariz",
    "a:nose-club_description": "Durante el día, puedes ponerte el dedo en la nariz discretamente y los otros tendrán que imitarte. El último en tocarse la nariz debe descartar sus habilidades sin revelarlas. 1 uso.",
    "a:president-call_name": "Llamada del presidente",
    "a:president-call_description": "Durante el día, decides cuando llama el presidente. El narrador te dirá una clave (palabra o frase) y os la vais a ir diciendo de oreja a oreja, cuando todos han escuchado la clave la irán diciendo uno a uno en voz alta y sentido inverso. Si la clave se ha transmitido correctamente, los hombres lobo no se despertarán la siguiente noche. 1 uso.",
    "a:vital-agreement_name": "Pacto vital",
    "a:vital-agreement_description": "Cuando alguien muere, si la otra persona con esta habilidad también quiere, podéis revivirla. 1 uso.",
    "a:ghost_name": "Ghost",
    "a:ghost_description": "Cada noche, escoje una persona para que el narrador lo asuste."
  },
